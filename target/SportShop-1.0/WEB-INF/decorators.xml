<decorators defaultdir="/WEB-INF/views/layouts">
	<decorator name="userLanding" page="user_landingPage.jsp">
		<pattern>/</pattern>
	</decorator>
	<decorator name="user" page="user_main.jsp">
		<pattern>/*</pattern>
